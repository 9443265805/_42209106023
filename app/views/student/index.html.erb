<h1>Listing Quiz</h1>
<table>
<tr>
<th>Quiz Name</th>
<th>Score</th>
<th></th>
</tr>
<% for studentquiz in @studentquizlist %>
<tr>
<td><%= studentquiz.quiz.quizname %></td>
<td><%= studentquiz.score %></td>
<td><%= link_to 'Take Quiz',student_startquiz_path(quiz_id:studentquiz.quiz_id) %></td>
</tr>
<% end %>
</table>