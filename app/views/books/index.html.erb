<h3>Books List </h3>
<table>
<% for book in @books %>
<tr>
<td><%=book.bookname %></td>
<td><%=book.author %></td>
<td><%=book.category %></td>
<td><%=book.price %></td>
<td><%=book.type %></td>
<td><%=book.duedate %></td>
<td><%=book.lenddate %></td>
<td><%=book.renewcount %></td>
<td><%=book.status %></td>
</tr>
<% end %>
</table>
